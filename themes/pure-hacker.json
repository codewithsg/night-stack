{
  "name": "Night-Stack Pure Hacker",
  "type": "dark",
  "semanticHighlighting": true,

  "colors": {

    "editor.background": "#000000",
    "editor.foreground": "#00FF00",
    "editorCursor.foreground": "#00FF00",
    "editor.lineHighlightBackground": "#003300",
    "editor.selectionBackground": "#005500",
    "editor.inactiveSelectionBackground": "#003300",

    "activityBar.background": "#000000",
    "activityBar.foreground": "#00FF00",
    "activityBar.inactiveForeground": "#008F11",

    "sideBar.background": "#000000",
    "sideBar.foreground": "#00FF00",
    "sideBar.border": "#003300",

    "statusBar.background": "#000000",
    "statusBar.foreground": "#00FF00",

    "titleBar.activeBackground": "#000000",
    "titleBar.activeForeground": "#00FF00",

    "panel.background": "#000000",
    "terminal.background": "#000000",
    "terminal.foreground": "#00FF00",

    "terminal.ansiBlack": "#000000",
    "terminal.ansiRed": "#FF4C4C",
    "terminal.ansiGreen": "#00FF00",
    "terminal.ansiYellow": "#39FF14",
    "terminal.ansiBlue": "#00FFFF",
    "terminal.ansiMagenta": "#FF00FF",
    "terminal.ansiCyan": "#00FFFF",
    "terminal.ansiWhite": "#FFFFFF"
  },

  "tokenColors": [

    {
      "scope": "comment",
      "settings": {
        "foreground": "#008F11",
        "fontStyle": "italic"
      }
    },

    {
      "scope": "string",
      "settings": {
        "foreground": "#39FF14"
      }
    },

    {
      "scope": "keyword",
      "settings": {
        "foreground": "#00FF00",
        "fontStyle": "bold"
      }
    },

    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#39FF14"
      }
    },

    {
      "scope": "variable",
      "settings": {
        "foreground": "#00FF00"
      }
    },

    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#39FF14"
      }
    }
  ],

  "semanticTokenColors": {
    "variable": "#00FF00",
    "function": "#39FF14",
    "method": "#39FF14",
    "string": "#39FF14",
    "number": "#39FF14",
    "keyword": "#00FF00",
    "type": "#00FF00",
    "class": "#00FF00",
    "invalid": "#FF4C4C"
  }
}
