{
  "name": "Night-Stack DOS Blue",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "editor.background": "#000080",
    "editor.foreground": "#FFFFFF",
    "editorCursor.foreground": "#FFFF00",
    "editor.lineHighlightBackground": "#000099",
    "editor.selectionBackground": "#0000CC",
    "editor.inactiveSelectionBackground": "#0000A0",
    "activityBar.background": "#000080",
    "activityBar.foreground": "#FFFFFF",
    "activityBar.inactiveForeground": "#C0C0C0",
    "sideBar.background": "#000090",
    "sideBar.foreground": "#FFFFFF",
    "sideBar.border": "#0000CC",
    "statusBar.background": "#000090",
    "statusBar.foreground": "#FFFF00",
    "titleBar.activeBackground": "#000090",
    "titleBar.activeForeground": "#FFFFFF",
    "panel.background": "#000080",
    "terminal.background": "#000080",
    "terminal.foreground": "#FFFFFF",
    "terminal.ansiBlack": "#000066",
    "terminal.ansiRed": "#FF0000",
    "terminal.ansiGreen": "#00FF00",
    "terminal.ansiYellow": "#FFFF00",
    "terminal.ansiBlue": "#0000FF",
    "terminal.ansiMagenta": "#FF00FF",
    "terminal.ansiCyan": "#00FFFF",
    "terminal.ansiWhite": "#FFFFFF"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#C0C0C0",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#FFFF00",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#00FF00"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#FF00FF"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#FFFF00"
      }
    }
  ],
  "semanticTokenColors": {
    "variable": "#FFFFFF",
    "function": "#00FF00",
    "method": "#00FF00",
    "string": "#00FFFF",
    "number": "#FF00FF",
    "keyword": "#FFFF00",
    "type": "#FFFF00",
    "class": "#FFFF00",
    "invalid": "#FF0000"
  }
}