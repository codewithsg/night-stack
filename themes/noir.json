{
  "name": "Night-Stack Noir",
  "type": "dark",
  "semanticHighlighting": true,

  "colors": {
    "editor.background": "#000000",
    "editor.foreground": "#E5E5E5",
    "editorCursor.foreground": "#FFFFFF",
    "editor.lineHighlightBackground": "#0A0A0A",
    "editor.selectionBackground": "#1A1A1A",
    "editor.inactiveSelectionBackground": "#111111",

    "activityBar.background": "#000000",
    "activityBar.foreground": "#CCCCCC",
    "activityBar.inactiveForeground": "#666666",

    "sideBar.background": "#050505",
    "sideBar.foreground": "#D4D4D4",
    "sideBar.border": "#111111",

    "statusBar.background": "#050505",
    "statusBar.foreground": "#CCCCCC",

    "titleBar.activeBackground": "#050505",
    "titleBar.activeForeground": "#D4D4D4",

    "panel.background": "#000000",
    "terminal.background": "#000000",
    "terminal.foreground": "#E5E5E5",

    "terminal.ansiBlack": "#0D0D0D",
    "terminal.ansiRed": "#999999",
    "terminal.ansiGreen": "#B3B3B3",
    "terminal.ansiYellow": "#CCCCCC",
    "terminal.ansiBlue": "#808080",
    "terminal.ansiMagenta": "#A6A6A6",
    "terminal.ansiCyan": "#D9D9D9",
    "terminal.ansiWhite": "#FFFFFF"
  },

  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#666666",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#BFBFBF"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#E5E5E5",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#E5E5E5"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#D9D9D9"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#B3B3B3"
      }
    }
  ],

  "semanticTokenColors": {
    "variable": "#E5E5E5",
    "function": "#CCCCCC",
    "method": "#CCCCCC",
    "string": "#BFBFBF",
    "number": "#D9D9D9",
    "keyword": "#E5E5E5",
    "type": "#B3B3B3",
    "class": "#B3B3B3",
    "invalid": "#FF4D4D"
  }
}