{
  "name": "Night-Stack Onyx",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "editor.background": "#020202",
    "editor.foreground": "#E5E5E5",
    "editorCursor.foreground": "#C0C0C0",
    "editor.lineHighlightBackground": "#0A0A0A",
    "editor.selectionBackground": "#1A1A1A",
    "editor.inactiveSelectionBackground": "#111111",
    "activityBar.background": "#020202",
    "activityBar.foreground": "#C0C0C0",
    "activityBar.inactiveForeground": "#666666",
    "sideBar.background": "#070707",
    "sideBar.foreground": "#D4D4D4",
    "sideBar.border": "#1A1A1A",
    "statusBar.background": "#070707",
    "statusBar.foreground": "#C0C0C0",
    "titleBar.activeBackground": "#070707",
    "titleBar.activeForeground": "#D4D4D4",
    "panel.background": "#020202",
    "terminal.background": "#020202",
    "terminal.foreground": "#E5E5E5",
    "terminal.ansiBlack": "#111111",
    "terminal.ansiRed": "#999999",
    "terminal.ansiGreen": "#C0C0C0",
    "terminal.ansiYellow": "#D4D4D4",
    "terminal.ansiBlue": "#808080",
    "terminal.ansiMagenta": "#A6A6A6",
    "terminal.ansiCyan": "#E5E5E5",
    "terminal.ansiWhite": "#FFFFFF"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#666666",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#C0C0C0"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#E5E5E5",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#E5E5E5"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#C0C0C0"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#D4D4D4"
      }
    }
  ],
  "semanticTokenColors": {
    "variable": "#E5E5E5",
    "function": "#D4D4D4",
    "method": "#D4D4D4",
    "string": "#C0C0C0",
    "number": "#C0C0C0",
    "keyword": "#E5E5E5",
    "type": "#D4D4D4",
    "class": "#D4D4D4",
    "invalid": "#FF4D4D"
  }
}