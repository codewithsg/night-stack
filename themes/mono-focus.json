{
  "name": "Night-Stack Mono Focus",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "editor.background": "#0A0A0A",
    "editor.foreground": "#E5E5E5",
    "editorCursor.foreground": "#FFFFFF",
    "editor.lineHighlightBackground": "#141414",
    "editor.selectionBackground": "#222222",
    "editor.inactiveSelectionBackground": "#1A1A1A",
    "activityBar.background": "#0A0A0A",
    "activityBar.foreground": "#CCCCCC",
    "activityBar.inactiveForeground": "#666666",
    "sideBar.background": "#0F0F0F",
    "sideBar.foreground": "#D4D4D4",
    "sideBar.border": "#222222",
    "statusBar.background": "#0F0F0F",
    "statusBar.foreground": "#CCCCCC",
    "titleBar.activeBackground": "#0F0F0F",
    "titleBar.activeForeground": "#D4D4D4",
    "panel.background": "#0A0A0A",
    "terminal.background": "#0A0A0A",
    "terminal.foreground": "#E5E5E5",
    "terminal.ansiBlack": "#111111",
    "terminal.ansiRed": "#888888",
    "terminal.ansiGreen": "#AAAAAA",
    "terminal.ansiYellow": "#CCCCCC",
    "terminal.ansiBlue": "#777777",
    "terminal.ansiMagenta": "#999999",
    "terminal.ansiCyan": "#DDDDDD",
    "terminal.ansiWhite": "#FFFFFF"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#666666",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#BBBBBB"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#E5E5E5",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#E5E5E5"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#DDDDDD"
      }
    }
  ],
  "semanticTokenColors": {
    "variable": "#E5E5E5",
    "function": "#CCCCCC",
    "method": "#CCCCCC",
    "string": "#BBBBBB",
    "number": "#CCCCCC",
    "keyword": "#E5E5E5",
    "type": "#DDDDDD",
    "class": "#DDDDDD",
    "invalid": "#FF4D4D"
  }
}