{
  "name": "Night-Stack Arctic Night",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "editor.background": "#060A10",
    "editor.foreground": "#E6F6FF",
    "editorCursor.foreground": "#7DEBFF",
    "editor.lineHighlightBackground": "#0F1622",
    "editor.selectionBackground": "#1B2A3E",
    "editor.inactiveSelectionBackground": "#141F2E",
    "activityBar.background": "#060A10",
    "activityBar.foreground": "#7DEBFF",
    "activityBar.inactiveForeground": "#4FA3B8",
    "sideBar.background": "#0B111A",
    "sideBar.foreground": "#D0EFFF",
    "sideBar.border": "#162334",
    "statusBar.background": "#0B111A",
    "statusBar.foreground": "#7DEBFF",
    "titleBar.activeBackground": "#0B111A",
    "titleBar.activeForeground": "#D0EFFF",
    "panel.background": "#060A10",
    "terminal.background": "#060A10",
    "terminal.foreground": "#E6F6FF",
    "terminal.ansiBlack": "#121A24",
    "terminal.ansiRed": "#8ED1FF",
    "terminal.ansiGreen": "#A4F4FF",
    "terminal.ansiYellow": "#C7F0FF",
    "terminal.ansiBlue": "#7DEBFF",
    "terminal.ansiMagenta": "#9ADFFF",
    "terminal.ansiCyan": "#B8F6FF",
    "terminal.ansiWhite": "#E6F6FF"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#6C8A9B",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#A4F4FF"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#7DEBFF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#8ED1FF"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#E6F6FF"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#C7F0FF"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#9ADFFF"
      }
    }
  ],
  "semanticTokenColors": {
    "variable": "#E6F6FF",
    "function": "#8ED1FF",
    "method": "#8ED1FF",
    "string": "#A4F4FF",
    "number": "#C7F0FF",
    "keyword": "#7DEBFF",
    "type": "#9ADFFF",
    "class": "#9ADFFF",
    "invalid": "#FF7A7A"
  }
}