{
  "name": "Night-Stack Obsidian",
  "type": "dark",
  "semanticHighlighting": true,

  "colors": {
    "editor.background": "#0B0E14",
    "editor.foreground": "#D6DEEB",
    "editorCursor.foreground": "#5DA9FF",
    "editor.lineHighlightBackground": "#141A22",
    "editor.selectionBackground": "#1E2633",
    "editor.inactiveSelectionBackground": "#171E28",

    "activityBar.background": "#0B0E14",
    "activityBar.foreground": "#7AA2F7",
    "activityBar.inactiveForeground": "#4C6FAE",

    "sideBar.background": "#0E131A",
    "sideBar.foreground": "#C0CAF5",
    "sideBar.border": "#1A2130",

    "statusBar.background": "#0E131A",
    "statusBar.foreground": "#7AA2F7",

    "titleBar.activeBackground": "#0E131A",
    "titleBar.activeForeground": "#C0CAF5",

    "panel.background": "#0B0E14",
    "terminal.background": "#0B0E14",
    "terminal.foreground": "#D6DEEB",

    "terminal.ansiBlack": "#1C1F26",
    "terminal.ansiRed": "#F7768E",
    "terminal.ansiGreen": "#9ECE6A",
    "terminal.ansiYellow": "#E0AF68",
    "terminal.ansiBlue": "#5DA9FF",
    "terminal.ansiMagenta": "#BB9AF7",
    "terminal.ansiCyan": "#7DCFFF",
    "terminal.ansiWhite": "#D6DEEB"
  },

  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#5C6370",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#9ECE6A"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#7AA2F7",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#7DCFFF"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#D6DEEB"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#E0AF68"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#BB9AF7"
      }
    }
  ],

  "semanticTokenColors": {
    "variable": "#D6DEEB",
    "function": "#7DCFFF",
    "method": "#7DCFFF",
    "string": "#9ECE6A",
    "number": "#E0AF68",
    "keyword": "#7AA2F7",
    "type": "#BB9AF7",
    "class": "#BB9AF7",
    "invalid": "#F7768E"
  }
}